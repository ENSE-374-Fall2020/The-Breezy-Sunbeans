    <!-- Custom styles for this template -->
    <link href="jumbotron.css" rel="stylesheet" />
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <a class="navbar-brand" href="#">Ocean</a>
      <button
        class="navbar-toggler"
        type="button"
        data-toggle="collapse"
        data-target="#navbarsExampleDefault"
        aria-controls="navbarsExampleDefault"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>

      <!-- Add any links you want to add-->
      <div class="collapse navbar-collapse" id="navbarsExampleDefault">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
            <a class="nav-link" href="/dashboard">Dashboard</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link" href="/profile">Profile</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link" href="/messaging">Messaging</a>
          </li>
        </ul>

        <form class="form-signin" action="/logout" method="GET">
          <button class="btn btn-secondary" type="submit">Log Out</button>
        </form>
      </div>
    </nav>
    <div class="jumbotron bg-primary text-white">
    <div class="container">
      <h1 class="display-3"><%= type %></h1>
      <p><%= date %></p>
      <% if ((username2 == "") && username1 != user){ %>
      <form class="form-update" action="/meetingJoin" method="POST">
        <input type="hidden" id="inputId" class="form-control" value="<%= _id %>" required name="_id">
        <input type="hidden" id="inputUserame2" class="form-control" value="<%= user %>" required name="username2">
        <p><button class="btn btn-primary btn-lg" type="submit">Join &raquo;</button></p> <!--Add any buttons and details/links-->
      </form>
      <% } %>
    </div>
  </div>
</div>

  <div class="container">
    <div class="row">
      <div class="col-md-4">
        <svg class="bd-placeholder-img mr-2 rounded" width="40" height="40" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: 32x32"><title>Placeholder</title><rect width="100%" height="100%" fill="#007bff"/><text x="50%" y="50%" fill="#007bff" dy=".3em">32x32</text></svg>
        <strong><%= user1Name %> </strong><br>
        <%= username1 %><br><br>
        <form action="/viewprofile" method="POST">
          <input type="hidden" name="username" value=<%= username1 %>>
          <input type="submit" class="btn btn-secondary" value="View <%= username1 %>'s profile!">
        </form>
      </div>
        
      <div class="col-md-4">
        <svg class="bd-placeholder-img mr-2 rounded" width="40" height="40" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: 32x32"><title>Placeholder</title><rect width="100%" height="100%" fill="#007bff"/><text x="50%" y="50%" fill="#007bff" dy=".3em">32x32</text></svg>
        <% if (username2 == ""){ %>
          <strong>Empty</strong>
          ---
        <% } else { %>
          <strong><%= user2Name %></strong><br>
          <%= username2 %><br><br>
          <form action="/viewprofile" method="POST">
            <input type="hidden" name="username" value=<%= username2 %>>
            <input type="submit" class="btn btn-secondary" value="View <%= username2 %>'s profile!">
          </form>
        <% } %>
    </div>

    <% if ((username1 == user) || (username2 == user)) { %>
    <form class="centred" action="/meetingEdit" method="POST">
      <input type="hidden" id="inputId" class="form-control" value="<%= _id %>" required name="_id">
      <button class="btn btn-secondary" type="submit">Edit</button>
  </form> 
  <% } %>
  </body>
</html>
