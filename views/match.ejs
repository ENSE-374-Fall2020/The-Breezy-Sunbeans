    <style>
      .bd-placeholder-img {
        font-size: 1.125rem;
        text-anchor: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
      }

      @media (min-width: 768px) {
        .bd-placeholder-img-lg {
          font-size: 3.5rem;
        }
      }
    </style>
    <!-- Custom styles for this template -->
    <link href="jumbotron.css" rel="stylesheet" />
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-light" style="background-color: lightskyblue;">
      <a class="navbar-brand" href="#">Ocean</a>
      <button
        class="navbar-toggler"
        type="button"
        data-toggle="collapse"
        data-target="#navbarsExampleDefault"
        aria-controls="navbarsExampleDefault"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>

      <!-- Add any links you want to add-->
      <div class="collapse navbar-collapse" id="navbarsExampleDefault">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
            <a class="nav-link" href="/dashboard">Dashboard</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link" href="/profile">Profile</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link" href="/messaging">Messaging</a>
          </li>
        </ul>

        <form class="form-signin" action="/logout" method="GET">
          <button class="btn btn-secondary" type="submit">Log Out</button>
        </form>
      </div>
    </nav>
    <% if (match == "on") { %>
    <div class="jumbotron bg-primary text-white">
    <div class="container">
      <h1 class="display-3">Your match</h1>
      <p>You and <%= userInfo2.name %> both like: <div class="text-capitalize"><%= interest %></div></p>
      <% if (userInfo.username != userInfo2.username) { %>
        <form class="form-update" action="/matchCreate" method="POST">
          <input type="hidden" id="inputUserame" class="form-control" value="<%= user %>" required name="username">
          <input type="hidden" id="inputUserame2" class="form-control" value="<%= userInfo2.username  %>" required name="username2">
          <input type="hidden" id="inputUserame" class="form-control" value="<%= interest %>" required name="interest">
          <p><button class="btn btn-primary btn-lg" type="submit">Make a meeting &raquo;</button></p> <!--Add any buttons and details/links-->
        </form>
      <% } %>
    </div>
  </div>
</div>

  <div class="container">
    <div class="row">
      <div class="col-md-4">
        <svg class="bd-placeholder-img mr-2 rounded" width="40" height="40" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: 32x32"><title>Placeholder</title><rect width="100%" height="100%" fill="#007bff"/><text x="50%" y="50%" fill="#007bff" dy=".3em">32x32</text></svg>
        <strong><%= userInfo.name %> </strong><br>
        <%= userInfo.username %><br><br>
        <form action="/viewprofile" method="POST">
          <input type="hidden" name="username" value=<%= userInfo.username %>>
          <input type="submit" class="btn btn-secondary" value="View <%= userInfo.username %>'s profile!">
        </form>
      </div>
        
      <div class="col-md-4">
        <svg class="bd-placeholder-img mr-2 rounded" width="40" height="40" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: 32x32"><title>Placeholder</title><rect width="100%" height="100%" fill="#007bff"/><text x="50%" y="50%" fill="#007bff" dy=".3em">32x32</text></svg>
          <strong><%= userInfo2.name %></strong><br>
          <%= userInfo2.username %><br><br>
          <% if (userInfo.username == userInfo2.username) { %>
              Oops this is you, <input class="btn btn-secondary" value="Try again" onClick="window.location.reload();">
            <% } else { %>
          <form action="/viewprofile" method="POST">
            <input type="hidden" name="username" value=<%= userInfo2.username %>>
            <input type="submit" class="btn btn-secondary" value="View <%= userInfo2.username %>'s profile!">
          </form>
          <% } %>
        </div>
    <% } else { %>
        NO interests listed to match with 
    <% } %>
  </body>
</html>
