            <h4 class="card-title">My Messages</h4>
            <div class="alert alert-dark">
               Bryden: <span class="bg-dark text-white">test test</span>
            </div>

            <div class="alert alert-dark">
                Albert: <span class="bg-dark text-white">Hello</span>
            </div>

            <div class="alert alert-dark">
                Messages with someone
            </div>

            <div class="alert alert-dark">
                Messages with someone
            </div>
        </div>
    </div>

  





